<body>
    <div class="todobox">
        <div class="todotop">
            <div class="text">
                ToDoList
            </div>

            <input type="text" placeholder="添加ToDo" name="" id="todoinput" value="" autofocus="autofocus" />

        </div>
        <button class="addlist">新增任务</button>
        <div class="todolist ongoing">
            <div class="listtop">
                <span>正在进行</span><span></span>
            </div>
            <div class="list list1">
                <ul id="ullist">
                    <!--<li><span><input type="checkbox" name="" id="" value="" style="zoom:180%"/>学习js</span><span class="del">X</span></li>
						-->
                </ul>
            </div>
        </div>
        <div class="todolist done">
            <div class="listtop">
                <span>已经完成</span><span></span>
            </div>
            <div class="list list2">
                <ul id="donelist">
                    <!--<li><span><input type="checkbox" name="" id="" value="" style="zoom:180%"/>完成学习js2</span><span class="del">X</span></li>-->
                </ul>
            </div>
        </div>
    </div>
</body>

</html>